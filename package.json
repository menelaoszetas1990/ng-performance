{
  "name": "performance",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "proxy": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:serve": "ng build --c production && serve dist/performance -s",
    "serve": "serve dist/performance -s",
    "caddy": "caddy start",
    "caddy:stop": "caddy stop",
    "stats": "ng b --c production --stats-json",
    "stats:wba": "ng b --c production --stats-json && webpack-bundle-analyzer dist/performance/stats.json",
    "wba": "webpack-bundle-analyzer dist/performance/stats.json",
    "build:sme": "ng b --c production && source-map-explorer dist/performance/**/*.js",
    "sme": "source-map-explorer dist/performance/**/*.js",
    "igv": "npx @rx-angular/import-graph-visualizer --entry-points src/main.ts --ts-config tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.7",
    "@angular/common": "15.2.7",
    "@angular/compiler": "15.2.7",
    "@angular/core": "15.2.7",
    "@angular/forms": "15.2.7",
    "@angular/platform-browser": "15.2.7",
    "@angular/platform-browser-dynamic": "15.2.7",
    "@angular/router": "15.2.7",
    "angular-oauth2-oidc": "^15.0.1",
    "anychart": "^8.11.0",
    "bootstrap": "^4.6.2",
    "moment": "^2.29.4",
    "ngx-quicklink": "^0.4.1",
    "rxjs": "^7.8.0",
    "tslib": "~2.3.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/language-service": "^15.2.7",
    "@briebug/jest-schematic": "^6.0.0",
    "@rx-angular/import-graph-visualizer": "^0.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "caddy": "^0.0.13",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "inquirer": "^6.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.8.3",
    "serve": "^14.2.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
